<canvas id="chart" phx-update="ignore" phx-hook="Chart" data-probes={"#{Jason.encode!(@probes)}"} width="100%" height="100%"></canvas>

<table>
  <thead>
    <tr>
      <th></th>
    </tr>
  </thead>
  <tbody id="probes">
    <%= for probe <- @probes do %>
      <tr id={"probe-#{probe.id}"}>

        <td>
          <span><%= probe.pm10 %></span>
          <span><%= probe.pm25 %></span>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
